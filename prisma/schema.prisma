generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Url {
  id           String   @id
  originalUrl  String
  shortCode    String   @unique
  clicks       Int      @default(0)
  createdAt    DateTime @default(now())
  expiresAt    DateTime?
}
